{
  "manifest_version": 3,
  "name": "No more Discounts!",
  "version": "1.0",
  "description": "No more Fake RRPs and Brand names at Bestsecret.com - Only real prices",
  "icons": {
    "48": "icons/icon48.png", 
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.bestsecret.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}